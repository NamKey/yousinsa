echo "---build start---"

stages
{
    stage('Git Pull Stage') {
        print('Git Pull Stage')



        print('Git Pull Stage END')
    }

    stage('Run Unit tests') {
        print('Run Unit Test Stage')

        print('Run Unit Test Stage END')
    }

    stage('Build Stage') {
        print('Build Stage')

        print('Build Stage END')
    }

    stage('Deploy Stage') {
        print('Deploy Stage')

        print('Deploy Stage END')
    }
}

void print(message) {
    echo "---${message}---"
}
